@model ICollection<AspMvcEf.Models.Test_Group_Ref>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout.cshtml";
    
}

<h2>Test Typy</h2>
@Html.ActionLink("Add", "AddEdit", null, new { id = 0 }, null)

<table >
    <tr>
        <th>-</th>
        <th>Id</th>
        <th>Test Type</th>
    </tr>
    @foreach (AspMvcEf.Models.Test_Group_Ref item in Model)
    {
        <tr>
            <td>@Html.ActionLink("edit", "AddEdit", null, new { id = item.Id}, null)</td>
            <td>@item.Id</td>
            <td>@item.Test_Group_Id</td>
            <td>@item.Test_Id</td>
        </tr>
    }
</table>