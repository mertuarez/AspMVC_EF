@model ICollection<AspMvcEf.Models.Test>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout.cshtml";
    
}

<h2>Testy</h2>

@Html.ActionLink("Add", "AddEdit", null, new { id = 0 }, null)

<table >
    <tr>
        <th>-</th>
        <th>-</th>
        <th>Id</th>
        <th>Nazov</th>
        <th>Typ Zmluvy</th>
        <th>Cislo Zmluvy</th>
    </tr>
    @foreach (AspMvcEf.Models.Test item in Model)
    {
        <tr>
            <td>@Html.ActionLink("edit", "AddEdit", null, new { id = item.Id}, null)</td>
            <td>@Html.ActionLink("del", "Remove", null, new { id = item.Id }, null)</td>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Test_Type.Name</td>
            <td>@item.Insurance_Id</td>
        </tr>
    }
</table>