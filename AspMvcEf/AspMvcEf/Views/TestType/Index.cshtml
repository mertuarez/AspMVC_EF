@model ICollection<AspMvcEf.Models.Test_Type>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout.cshtml";
    
}

<h2>Test Typy</h2>
@Html.ActionLink("Add", "AddEdit", null, new { id = 0 }, null)

<table >
    <tr>
        <th>-</th>
        <th>-</th>
        <th>Id</th>
        <th>Type</th>
        <th>Name</th>
    </tr>
    @foreach (AspMvcEf.Models.Test_Type item in Model)
    {
        <tr>
            <td>@Html.ActionLink("edit", "AddEdit", null, new { id = item.Id}, null)</td>
            <td>@Html.ActionLink("del", "Remove", null, new { id = item.Id }, null)</td>
            <td>@item.Id</td>
            <td>@item.Type</td>
            <td>@item.Name</td>
        </tr>
    }
</table>